{
    "name": "fligno/fligno-toolkit",
    "description": "Fligno Toolkit Laravel Package",
    "license": "MIT",
    "authors": [
        {
            "name": "James Carlo Luchavez",
            "email": "jamescarlo.luchavez@fligno.com",
            "homepage": "https://www.linkedin.com/in/jsluchavez/"
        }
    ],
    "homepage": "https://github.com/fligno/fligno-toolkit",
    "keywords": ["Laravel", "FlignoToolkit"],
    "require": {
        "illuminate/support": "~7|~8",
        "fligno/gitlab-sdk": "^1.0.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "files": [
            "helpers/CustomHelpers.php"
        ],
        "psr-4": {
            "Fligno\\FlignoToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fligno\\FlignoToolkit\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fligno\\FlignoToolkit\\FlignoToolkitServiceProvider"
            ],
            "aliases": {
                "FlignoToolkit": "Fligno\\FlignoToolkit\\Facades\\FlignoToolkit"
            }
        }
    }
}
